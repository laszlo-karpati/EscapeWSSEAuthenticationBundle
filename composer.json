{
    "name": "escapestudios/wsse-authentication-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle to implement WSSE authentication, with FOSUserbundle as user provider and symfony encode as password encoder",
    "keywords": ["wsse", "authentication", "bundle", "FOSUserbundle"],
    "homepage": "https://github.com/escapestudios/EscapeWSSEAuthenticationBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David Joos",
            "email": "david@escapestudios.com"
        },
        {
            "name": "Ron Hamersma",
            "email": "ron@hintt.nl"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/escapestudios/EscapeWSSEAuthenticationBundle/graphs/contributors"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/framework-bundle": "2.2.*",
        "symfony/security-bundle": "2.2.*"
    },
    "autoload": {
        "psr-0": { "escapestudios\\WSSEAuthenticationBundle": "" }
    },
    "target-dir": "escapestudios/WSSEAuthenticationBundle"
}
